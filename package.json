  "dependencies": {
    "@nomiclabs/hardhat-truffle5": "^2.0.5",
    "hardhat": "^2.9.3"
  },
  "scripts": {
    "compile": "cross-env SKIP_LOAD=true hardhat compile --force && yarn gen",
    "test": "yarn compile && hardhat test",
    "gen": "cross-env SKIP_LOAD=true hardhat typechain",
    "deploy:mainnet": "yarn compile && hardhat --network mainnet deploy --wait 3",
    "deploy:sepolia": "yarn compile && hardhat --network sepolia deploy --wait 3",
    "deploy:holesky": "yarn compile && hardhat --network holesky deploy --wait 3",
    "deploy:baseSepolia": "yarn compile && hardhat --network baseSepolia deploy --wait 3",
    "deploy:base": "yarn compile && hardhat --network base deploy --wait 3",
    "deploy:scrollSepolia": "yarn compile && hardhat --network scrollSepolia deploy --wait 3",
    "deploy:scroll": "yarn compile && hardhat --network scroll deploy --wait 3",
    "deploy:arbitrumSepolia": "yarn compile && hardhat --network arbitrumSepolia deploy --wait 3",
    "deploy:arbitrum": "yarn compile && hardhat --network arbitrum deploy --wait 3",
    "deploy:mantleSepolia": "yarn compile && hardhat --network mantleSepolia deploy --wait 3",
    "deploy:mantle": "yarn compile && hardhat --network mantle deploy --wait 3",
    "deploy:polygonZkevm": "yarn compile && hardhat --network polygonZkevm deploy --wait 3",
    "deploy:berachainBartio": "yarn compile && hardhat --network berachainBartio deploy --wait 3",
    "deploy:bevmTestnet": "yarn compile && hardhat --network bevmTestnet deploy --wait 3",
    "deploy:bitlayerTestnet": "yarn compile && hardhat --network bitlayerTestnet deploy --wait 3",
    "deploy:lineaSepolia": "yarn compile && hardhat --network lineaSepolia deploy --wait 3",
    "deploy:defiverseTestnet": "yarn compile && hardhat --network defiverseTestnet deploy --wait 3",
    "deploy:defiverseMainnet": "yarn compile && hardhat --network defiverseMainnet deploy --wait 3",
    "deploy:defiverse:testnet": "npx hardhat run --network defiverseTestnet scripts/deploy.js",
    "deploy:defiverse:mainnet": "npx hardhat run --network defiverseMainnet scripts/deploy.js"
  }
}
